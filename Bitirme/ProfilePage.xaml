                </AbsoluteLayout>
                
                
                <AbsoluteLayout AbsoluteLayout.LayoutBounds="0,170,1,50" AbsoluteLayout.LayoutFlags="WidthProportional" Padding="5,0,0,0">
                    <Label Text="City:" TextColor="#BB8C4B"
                        AbsoluteLayout.LayoutBounds="0,25,.25,50" AbsoluteLayout.LayoutFlags="WidthProportional" Opacity="40"/>
